             <form action="/" method="before">
          <div id="custom-search-input" class="input-group col-md-12">
            <input onFocus="this.select()" name="tag" type="text" class="  search-query form-control" placeholder="tags" />
            <span class="input-group-btn">
              <span class="input-group-btn">
                <button class="btn btn-primary" type="button">
                  <span class=" glyphicon glyphicon-search"></span>
                </button>
              </span>
            </div>
          </div>
         </form>
      </div>
    <section>
    <center>
    <ul id='links'>
    <% @data.each do |data| %>
      <% if link = data['link'] %>
      <li>
        <a href="<%=link%>" target="_blank" style=
        "visibility: visible; display: inline;"><img src=
        "<%=data['low']%>" /></a>
      </li>
     <% end %>
     <% end %>
    </section></ul>
